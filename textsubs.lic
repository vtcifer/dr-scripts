# quiet
=begin
  Documentation: https://elanthipedia.play.net/Lich_script_repository#textsubs
=end

custom_require.call(%w[drinfomon])

no_pause_all
no_kill_all

class TextSubs
  @@subs = {}

  def self.subs
    @@subs
  end

  def self.clear
    @@subs = {}
  end

  def self.add(target, result)
    @@subs[/#{target}/] = result
  end
end

settings = get_settings

action = proc do |client_string|
  matches = TextSubs.subs.select { |key, _| key =~ client_string }
  matches.each { |target, result| client_string.gsub!(target, result) }
  client_string
end

DownstreamHook.remove('textsub')
DownstreamHook.add('textsub', action)

before_dying do
  DownstreamHook.remove('textsub')
end

# Put shit list players in textsubs
if settings.integrate_shit_list_with_textsubs && File.file?("#{checkname}_shit.list")
  File.open("#{checkname}_shit.list", 'r') do |file|
    file.each do |line|
      name = line.split(',').first.capitalize
      TextSubs.add(name.to_s, "#{name} (SL)")
    end
  end
end

unless settings.private_textsubs.empty?
  settings.private_textsubs.each do |k, v|
    TextSubs.add(k, v)
  end
end

